pipeline
  agent any

  stages {
    stage('Hello') {
      steps {
        echo 'Hello World'
      }
    }
    stage('test') {
      steps {
        echo 'Super, le test est concluant !'
      }
    }
    stage('build') {
      steps {
        echo 'Build effectue'
      }
    }
    stage('deploy') {
      steps {
        echo 'Deploiement termine'
      }
    }
  }
